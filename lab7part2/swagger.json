{
    "swagger": "2.0",
    "info": {
      "version": "1.0.0",
      "title": "My Online Store Project",
      "description": "My User Project Application API",
      "license": {
        "name": "MIT",
        "url": "https://opensource.org/licenses/MIT"
      }
    },
    "host": "localhost:3007",
    "basePath": "/",
    "tags": [
      {
        "name": "Store",
        "description": "API for Calculus in the system"
      }
    ],
    "schemes": ["http"],
    "consumes": ["application/json"],
    "produces": ["application/json"],
    "paths": {
      "store/products": {
        "get": {
          "tags": ["Store Products"],
          "summary": "All of our products for sale",
          "parameters": [
            {
            "title": "item.title",
            "price": "item.price",
            "description":  "item.description",
            "image": "item.image"
            }
          ],
          "responses": {
            "200": {
              "description": "This service allows you to see products in our store."
            }
          }
        }
      }      
    }
}